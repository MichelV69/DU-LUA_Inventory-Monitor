{
  "cli": {
    "fmtVersion": 3
  },
  "name": "Inventory-Monitor",
  "description": "Simple Tool for Watching When Inventory Items Hit Historic Highs or Lows",
  "sourcePath": "src",
  "outputPath": "out",
  "libs": [],
  "builds": {
    "main": {
      "name": "main",
      "type": "control",
      "slots": {
        "db1": {
          "name": "db1",
          "type": "databank"
        }
      }
    }
  },
  "targets": {
    "development": {
      "name": "development",
      "minify": false,
      "handleErrors": false
    }
  },
  "internalPaths": [
    "autoconf/",
    "cpml/",
    "pl/",
    "utils/event"
  ]
}